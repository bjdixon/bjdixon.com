{{> header-notes}}
    <div class="container">

      <div class="row">

        <div class="col-sm-8 blog-main">

        {{#each collections.posts}}
            <div class="blog-post">
            <h2 class="blog-post-title">{{this.title}}</h2>
            <p class="blog-post-meta">{{formatDate this.date}} by <a href="{{this.authorUrl}}">{{this.author}}</a></p>
            {{{ellipsis this.contents}}} <a href="/{{this.path}}">Read more</a>
            </div>
        {{/each}}

        </div>

        <div class="col-sm-3 col-sm-offset-1 blog-sidebar">
                  <div class="sidebar-module sidebar-module-inset">
                    <h4>About</h4>
                    <p>Ramblings mostly about JavaScript or Python.</p>
          </div>
          <div class="sidebar-module">
            <h4>Elsewhere</h4>
            <ul class="list-unstyled">
              <li><a href="https://github.com/bjdixon">GitHub</a></li>
              <li><a href="https://gist.github.com/bjdixon">Gists</a></li>
            </ul>
          </div>
        </div><!-- /.blog-sidebar -->

      </div><!-- /.row -->

    </div><!-- /.container -->

{{> footer}}
